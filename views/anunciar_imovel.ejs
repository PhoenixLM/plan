.checks input[type=radio] + label,
.checks input[type=checkbox] + label {
    /* margin: 0 0 0 20px; */
    position: relative;
    cursor: pointer;
    float: left;
    font-size: 12px;
    font-family: "dinmedium";
    padding-top: 0px;
}

.checks input[type=radio] + label ~ label,
.checks input[type=checkbox] + label ~ label {
    margin: 0 0 0 40px;
}

.checks input[type=radio] + label::before,
.checks input[type=checkbox] + label::before {
    content: ' ';
    position: absolute;
    left: -27px;
    top: 2px;
    width: 15px;
    height: 15px;
    display: block;
    background: url("../images/nova-busca-checkbox.png") no-repeat;
    /* border: 1px solid #A9A9A9; */
}

.checks input[type=radio] + label::after,
.checks input[type=checkbox] + label::after {
    content: ' ';
    position: absolute;
    left: -26px;
    top: 4px;
    width: 13px;
    height: 10px;
    display: block;
    z-index: 1;
    background: url('../images/tik.svg') no-repeat center center;
    -ms-transition: all .2s ease;
    -webkit-transition: all .2s ease;
    transition: all .3s ease;
    -ms-transform: scale(0);
    -webkit-transform: scale(0);
    transform: scale(0);
    opacity: 0;
}

.checks input[type=radio]:checked + label::after,
.checks input[type=checkbox]:checked + label::after {
    -ms-transform: scale(1);
    -webkit-transform: scale(1);
    transform: scale(1);
    opacity: 1;
}

